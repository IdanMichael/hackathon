{
  "name": "Bring back dislikes",
  "description": "Returns ability to see dislikes",
  "version": "1.0.0",
  "manifest_version": 2,

  
  
 
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js" : ["main.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action":{
    "default_popup": "popup.html",
    "default_title": "Add dislikes"
  },
  "permissions":[
    "storage"
  ]
}